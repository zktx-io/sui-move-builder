{
  "name": "@zktx.io/sui-move-builder",
  "version": "0.1.8",
  "description": "Build Move packages in web or Node.js with dependency fetching and dump outputs.",
  "keywords": [
    "sui",
    "move",
    "compiler",
    "builder",
    "wasm"
  ],
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/zktx-io/sui-move-builder.git"
  },
  "homepage": "https://github.com/zktx-io/sui-move-builder",
  "bugs": {
    "url": "https://github.com/zktx-io/sui-move-builder/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/full/index.d.ts",
      "import": "./dist/full/index.js",
      "require": "./dist/full/index.cjs"
    },
    "./lite": {
      "types": "./dist/lite/index.d.ts",
      "import": "./dist/lite/index.js",
      "require": "./dist/lite/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "build:wasm": "node scripts/build-wasm.mjs",
    "build:js": "tsup --out-dir dist/full && tsup --out-dir dist/lite",
    "build": "npm run build:wasm && npm run build:js",
    "serve:test": "python3 -m http.server 8000 --directory .",
    "test:integration": "node test/integration/fidelity_test.mjs",
    "test:lite": "node test/integration/fidelity_test_lite.mjs",
    "test": "npm run test:integration",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "tsup": "^8.0.1",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.26.0"
  }
}